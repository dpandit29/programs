class Solution {
public:
    string reverseWords(string s) {
        int sa=s.size();
        cout<<sa;
         stringstream ss(s);
   string sam;
    string word;
    int i=0;
    int count = 0;
    while (ss >> word)
    {  if(i!=0)
        sam=word+" " + sam;
        else
        sam=word+sam;

        i++;
        
        }
    

       return sam; 
    }
};
